{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:recommended"
  ],
  "automerge": true,
  "automergeType": "branch",
  "baseBranches": ["main"],
  "branchConcurrentLimit": 7,
  "packageRules": [
    {
      "enabled": false,
      "matchPackageNames": ["*"]
    },
    {
      "matchBaseBranches": ["main"],
      "matchPackageNames": ["software.amazon.awssdk:s3"],
      "enabled": true,
      "prPriority": 2,
      "matchUpdateTypes": "patch"
    }
  ]
}
