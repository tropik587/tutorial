{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:recommended"
  ],
  "automerge": true,
  "prHourlyLimit": 50,
  "automergeType": "branch",
  "baseBranches": ["main"],
  "packageRules": [
    {
      "enabled": false,
      "matchPackageNames": ["*"]
    },
    {
      "matchBaseBranches": ["main"],
      "matchPackageNames": ["software.amazon.awssdk:s3"],
      "matchManagers": ["maven"],
      "allowedVersions": "/^[2]\\.[29]\\.[0-9]+(\\.[0-9]+)?$/",
      "enabled": true
    }
  ]
}
